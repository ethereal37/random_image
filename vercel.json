{
    "version": 2,
    "builds": [
      {
        "src": "api/random_image.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/random_image.py",
        "headers": {
          "Cache-Control": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0"
        }
      }
    ]
  }
  